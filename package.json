{
  "name": "electron-elm-boilerplate",
  "productName": "ElectronElm",
  "version": "0.0.1",
  "description": "Electron application boilerplate based on Elm & Webpack for rapid, staticly-typed application development",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint": "eslint app test *.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.development",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && elm package install",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaiyote/electron-elm-boilerplate.git"
  },
  "author": {
    "name": "Tim Huddle",
    "email": "tim.huddle88@gmail.com",
    "url": "https://github.com/kaiyote"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaiyote/electron-elm-boilerplate/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "webpack",
    "elm"
  ],
  "homepage": "https://github.com/kaiyote/electron-elm-boilerplate#readme",
  "devDependencies": {
    "asar": "^0.11.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "co-mocha": "^1.1.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "del": "^2.2.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.6",
    "electron-rebuild": "^1.1.3",
    "elm-hot-loader": "^0.2.2",
    "elm-webpack-loader": "https://github.com/rtfeldman/elm-webpack-loader",
    "eslint": "^2.8.0",
    "express": "^4.13.4",
    "fbjs-scripts": "^0.6.0",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "selenium-webdriver": "^2.53.1",
    "sinon": "^1.17.3",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "electron-debug": "^0.6.0",
    "font-awesome": "^4.6.1",
    "source-map-support": "^0.4.0"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  }
}
